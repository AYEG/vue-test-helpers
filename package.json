{
  "name": "@laura-wert/vue-test-helpers",
  "version": "2.2.1",
  "description": "",
  "productName": "",
  "license": "MIT",
  "author": " ",
  "main": "dist/index.umd.min.js",
  "typings": "lib/src/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn lint && yarn clean && yarn build:ts && yarn build:main",
    "build:ts": "tsc -p . --declaration --emitDeclarationOnly --allowJs false --noImplicitAny false && rm -rf ./lib/tests",
    "build:main": "vue-cli-service build --target lib --name org/index src/index.ts && mv ./dist/org/index.umd.min.js ./dist/ && rm -rf ./dist/org && rm -rf ./dist/demo.html",
    "clean": "rimraf ./dist && rimraf ./lib",
    "release": "yarn git-clean-status &&  yarn build && yarn publish",
    "lint": "vue-cli-service lint --fix && laura-wert-cli tslint",
    "test:unit": "vue-cli-service test:unit --include tests/unit/setup.ts tests/unit/",
    "test:unit-s": "vue-cli-service test:unit --include tests/unit/setup.ts",
    "test-ci": "yarn cover && cat ./coverage/lcov.info | yarn codacy-coverage",
    "cover": "cross-env NODE_ENV=coverage nyc --reporter=lcovonly yarn test:unit"
  },
  "peerDependencies": {
    "@vue/test-utils": ">=1.0.0-beta.12",
    "quasar": "^1.0.0-beta.1",
    "vue": ">=2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@laura-wert/git-clean-status": "^1.0.0",
    "@laura-wert/vue-helpers": "^1.0.10",
    "@types/chai": "^4.1.7",
    "@types/jquery": "^3.3.29",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "@types/sinon": "^7.0.6",
    "@types/sinon-chai": "^3.2.2",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-typescript": "^3.4.0",
    "@vue/cli-plugin-unit-mocha": "^3.4.0",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "axios": "^0.18.0",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "chai": "^4.2.0",
    "codacy-coverage": "^3.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^7.3.0",
    "nyc": "^13.1.0",
    "quasar": "^1.0.0-beta.5",
    "rimraf": "^2.6.2",
    "sinon": "^7.2.3",
    "sinon-chai": "^3.3.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "~3.2.2",
    "vue": ">=2.5.0",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.2.0",
    "vue-template-compiler": ">=2.5.17",
    "webpack-cli": "^3.1.2"
  },
  "files": [
    "package.json",
    "dist/",
    "lib/"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "vue-cli-service lint",
      "laura-wert-cli tslint",
      "git add"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "include": [
      "src/**/*.{js,ts,vue}"
    ],
    "reporter": [
      "lcov"
    ],
    "extension": [
      ".js",
      ".ts",
      ".vue"
    ],
    "cache": true,
    "all": true
  },
  "dependencies": {}
}
